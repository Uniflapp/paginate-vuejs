{
  "name": "paginate-vuejs",
  "version": "1.0.3",
  "type": "module",
  "workspaces": ["packages/*"],
  "description": "Paginate Vuejs is a Vue.js pagination component that provides customizable pagination styles.",
  "files": [
    "dist"
  ],
  "main": "./dist/paginate-vuejs.cjs",
  "module": "./dist/paginate-vuejs.js",
  "exports": {
    ".": {
      "import": "./dist/paginate-vuejs.js",
      "require": "./dist/paginate-vuejs.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "env.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uniflapp/paginate-vuejs.git"
  },
  "keywords": [
    "vue",
    "paginate",
    "pagination"
  ],
  "author": "Dadda Abdelghafour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Uniflapp/paginate-vuejs/issues"
  },
  "homepage": "https://github.com/Uniflapp/paginate-vuejs#readme",
  "publishConfig": {
    "@Uniflapp:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}